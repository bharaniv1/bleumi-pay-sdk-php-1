{
    "name": "bleumi/bleumi-pay-sdk-php",
    "version": "1.0.2",
    "description": "PHP SDK for Bleumi Pay REST API",
    "keywords": [
        "swagger",
        "php",
        "sdk",
        "api"
    ],
    "homepage": "http://pay.bleumi.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Thirupathy Shanmugam",
            "email": "thirupathy.shanmugam@bleumi.com",
            "homepage": "https://github.com/bleumi/bleumi-pay-sdk-php", 
            "role": "Developer"
        },
        {
            "name": "Karthic Kumaran Krisnakumar",
            "email": "karthic.krisnakumar@bleumi.com",
            "homepage": "https://github.com/bleumi/bleumi-pay-sdk-php", 
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~1.12"
    },
    "autoload": {
        "psr-4": { "Bleumi\\Pay\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Bleumi\\Pay\\" : "test/" }
    },
    "support": {
        "email": "support@bleumi.com"
    }
}
